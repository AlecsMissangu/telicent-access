# #!/usr/bin/env bash
set -e
cd frontend

# Function deletes tmp file
cleanup() {
  rm -f accessfrontend.sbom.json
}
# call the cleanup() on EXIT
trap cleanup EXIT

# Create temp file
echo "{}" > accessfrontend.sbom.json

# To debug it helps to use:  --no-cache \
docker build \
    --no-cache \
    --progress=plain \
    -f ./Dockerfile \
    -t telicent-access-ui-local \
    --load .
